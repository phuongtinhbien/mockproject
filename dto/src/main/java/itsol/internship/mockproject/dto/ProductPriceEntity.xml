            <basic name="vat">
                <column name="VAT"/>
            </basic>
            <basic name="staffCreate">
                <column name="STAFF_CREATE"/>
            </basic>
            <basic name="updatedDate">
                <column name="UPDATED_DATE"/>
            </basic>
            <basic name="staffUpdate">
                <column name="STAFF_UPDATE"/>
            </basic>
            <basic name="fromDate">
                <column name="FROM_DATE"/>
            </basic>
            <basic name="toDate">
                <column name="TO_DATE"/>
            </basic>
        </attributes>
