<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="itsol.internship.mockproject.dto.PermissionEntity">
        <table name="PERMISSION" schema="INTERN" catalog=""/>
        <attributes>
            <id name="permissionCode">
                <column name="PERMISSION_CODE"/>
            </id>
            <basic name="permissionId">
                <column name="PERMISSION_ID"/>
            </basic>
            <basic name="name">
                <column name="NAME"/>
            </basic>
            <basic name="content">
                <column name="CONTENT"/>
            </basic>
            <basic name="createdDate">
                <column name="CREATED_DATE"/>
            </basic>
            <basic name="staffCreate">
                <column name="STAFF_CREATE"/>
            </basic>
            <basic name="updateDate">
                <column name="UPDATE_DATE"/>
            </basic>
            <basic name="staffUpdate">
                <column name="STAFF_UPDATE"/>
            </basic>
            <basic name="status">
                <column name="STATUS"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
