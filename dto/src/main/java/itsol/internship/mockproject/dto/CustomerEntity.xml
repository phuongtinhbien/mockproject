<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="itsol.internship.mockproject.dto.CustomerEntity">
        <table name="CUSTOMER" schema="INTERN" catalog=""/>
        <attributes>
            <id name="customerCode">
                <column name="CUSTOMER_CODE"/>
            </id>
            <basic name="customerId">
                <column name="CUSTOMER_ID"/>
            </basic>
            <basic name="customerType">
                <column name="CUSTOMER_TYPE"/>
            </basic>
            <basic name="customerName">
                <column name="CUSTOMER_NAME"/>
            </basic>
            <basic name="gender">
                <column name="GENDER"/>
            </basic>
            <basic name="email">
                <column name="EMAIL"/>
            </basic>
            <basic name="password">
                <column name="PASSWORD"/>
            </basic>
            <basic name="dateOfBirth">
                <column name="DATE_OF_BIRTH"/>
            </basic>
            <basic name="createdDate">
                <column name="CREATED_DATE"/>
            </basic>
            <basic name="updatedDate">
                <column name="UPDATED_DATE"/>
            </basic>
            <basic name="staffUpdate">
                <column name="STAFF_UPDATE"/>
            </basic>
            <basic name="hashString">
                <column name="HASH_STRING"/>
            </basic>
            <basic name="lockStatus">
                <column name="LOCK_STATUS"/>
            </basic>
            <basic name="numLoginFail">
                <column name="NUM_LOGIN_FAIL"/>
            </basic>
            <basic name="searchName">
                <column name="SEARCH_NAME"/>
            </basic>
            <basic name="avatar">
                <column name="AVATAR"/>
            </basic>
            <basic name="lastTimeAccess">
                <column name="LAST_TIME_ACCESS"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
