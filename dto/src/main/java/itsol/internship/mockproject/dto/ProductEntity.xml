<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="itsol.internship.mockproject.dto.ProductEntity">
        <table name="PRODUCT" schema="INTERN" catalog=""/>
        <attributes>
            <id name="productCode">
                <column name="PRODUCT_CODE"/>
            </id>
            <basic name="productId">
                <column name="PRODUCT_ID"/>
            </basic>
            <basic name="productName">
                <column name="PRODUCT_NAME"/>
            </basic>
            <basic name="productTypeCode">
                <column name="PRODUCT_TYPE_CODE"/>
            </basic>
            <basic name="status">
                <column name="STATUS"/>
            </basic>
            <basic name="staffCreate">
                <column name="STAFF_CREATE"/>
            </basic>
            <basic name="createdDate">
                <column name="CREATED_DATE"/>
            </basic>
            <basic name="staffUpdate">
                <column name="STAFF_UPDATE"/>
            </basic>
            <basic name="updatedDate">
                <column name="UPDATED_DATE"/>
            </basic>
            <basic name="searchName">
                <column name="SEARCH_NAME"/>
            </basic>
            <basic name="shortDescription">
                <column name="SHORT_DESCRIPTION"/>
            </basic>
            <basic name="detailDescription">
                <column name="DETAIL_DESCRIPTION"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
