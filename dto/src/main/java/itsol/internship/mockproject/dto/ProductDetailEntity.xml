<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="itsol.internship.mockproject.dto.ProductDetailEntity">
        <table name="PRODUCT_DETAIL" schema="INTERN" catalog=""/>
        <attributes>
            <id name="productDetailId">
                <column name="PRODUCT_DETAIL_ID"/>
            </id>
            <basic name="productCode">
                <column name="PRODUCT_CODE"/>
            </basic>
            <basic name="productPriceId">
                <column name="PRODUCT_PRICE_ID"/>
            </basic>
            <basic name="imgCode">
                <column name="IMG_CODE"/>
            </basic>
            <basic name="colorCode">
                <column name="COLOR_CODE"/>
            </basic>
            <basic name="productSize">
                <column name="PRODUCT_SIZE"/>
            </basic>
            <basic name="weight">
                <column name="WEIGHT"/>
            </basic>
            <basic name="supplier">
                <column name="SUPPLIER"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
