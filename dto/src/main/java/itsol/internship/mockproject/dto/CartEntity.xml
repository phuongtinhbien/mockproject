<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="itsol.internship.mockproject.dto.CartEntity">
        <table name="CART" schema="INTERN" catalog=""/>
        <attributes>
            <id name="cartCode">
                <column name="CART_CODE"/>
            </id>
            <basic name="cartId">
                <column name="CART_ID"/>
            </basic>
            <basic name="customerCode">
                <column name="CUSTOMER_CODE"/>
            </basic>
            <basic name="productDetailId">
                <column name="PRODUCT_DETAIL_ID"/>
            </basic>
            <basic name="quantity">
                <column name="QUANTITY"/>
            </basic>
            <basic name="status">
                <column name="STATUS"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
